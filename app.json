{
  "expo": {
    "name": "SmartParking",
    "slug": "SmartParking",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/465abb021f7ccecbf70e8303bed15887815b29fb.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icons/465abb021f7ccecbf70e8303bed15887815b29fb.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.smartparking.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/465abb021f7ccecbf70e8303bed15887815b29fb.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.smartparking.app",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "entryPoint": "./src/app/App.js"
    },
    "scheme": "smartparking",
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-dev-client",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "This app uses location to find nearby parking spots and provide navigation.",
          "locationAlwaysPermission": "This app uses location to find nearby parking spots and provide navigation.",
          "locationWhenInUsePermission": "This app uses location to find nearby parking spots and provide navigation."
        }
      ]
    ],
    "extra": {
      "googleSignIn": {
        "webClientId": "956536297797-40t8l9ej41b1pqn7089929lc8iok2he4.apps.googleusercontent.com",
        "androidClientId": "956536297797-b0cb77s0ke877edooae4702dacnk9p32.apps.googleusercontent.com",
        "iosClientId": "956536297797-7rc1kpld9ljcdhkgmllhgp4urlqo5a9n.apps.googleusercontent.com"
      },
      "router": {},
      "eas": {
        "projectId": "ccd5fca9-4c6f-4456-8f38-ba4ce81ee769"
      }
    }
  }
}
